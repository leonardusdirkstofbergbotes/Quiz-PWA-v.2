"/\"> <!-- Home link -->\n          <v-list-item-action>\n            <v-icon>fa-home</v-icon>\n          </v-list-item-action>\n          <v-list-item-content>\n            <v-list-item-title>Home</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n\n        <v-list-item v-if=\"signedIn == false\" link to=\"/register\"> <!-- Register user -->\n          <v-list-item-action>\n            <v-icon>fa-user-plus</v-icon>\n          </v-list-item-action>\n          <v-list-item-content>\n            <v-list-item-title>Register</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n\n        <v-list-item v-if=\"signedIn == false\" link to=\"/login\"> <!-- Login user -->\n          <v-list-item-action>\n            <v-icon>fa-user-check</v-icon>\n          </v-list-item-action>\n          <v-list-item-content>\n            <v-list-item-title>Login</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n\n        <v-divider></v-divider>\n\n        <v-list-item v-for=\"item in sidebarOptions\" :key=\"item.name\" link :to=\"item.link\"> <!-- Link to categories -->\n          <v-list-item-content>\n            <v-list-item-title>{{item.name}}</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n\n      </v-list> <!-- Side bar ends -->\n    </v-navigation-drawer>\n\n    <v-app-bar app color=\"indigo\" dark dense>\n      <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\n      <v-toolbar-title>Application</v-toolbar-title>\n      <v-spacer></v-spacer>\n\n       <!-- If user is nog yet signed in -->\n        <v-btn v-if=\"user == false\" text to=\"/register\">Register<v-icon>fa-user-plus</v-icon></v-btn>\n        <v-btn v-if=\"user == false\" text to=\"/login\">Login<v-icon>fa-user-check</v-icon></v-btn>\n     \n\n      <v-btn text v-if=\"user !== false\"> <!-- If user is signed in -->\n        Welcome {{user.name}}\n      </v-btn>\n    </v-app-bar>\n\n    <v-main>\n      <v-container fluid>\n        <router-view></router-view>\n      </v-container>\n    </v-main>\n    <v-footer color=\"indigo\" app>\n      <span class=\"white--text\">&copy; {{ new Date().getFullYear() }}</span>\n    </v-footer>\n  </v-app>\n</template>\n\n\n<script>\n  export default {\n    name: 'main app',\n    data: () => ({\n      signedIn: false,\n      drawer: null,\n      sidebarOptions: [\n        {name: 'Profile', link: 'profile', requireSignIn: true}\n      ]\n    }),\n\n    computed: {\n      user () {\n        return this.$store.getters.getUser\n      }\n    },\n\n    watch: {\n      user (value) {\n        console.log(value) \n        if (value !== null) {\n          this.signedIn = true\n        }\n      }\n    }\n  }\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n\n#nav {\n  padding: 30px;\n}\n\n#nav a {\n  font-weight: bold;\n  color: #2c3e50;\n}\n\n#nav a.router-link-exact-active {\n  color: #42b983;\n}\n\n#loadingCircle {\n  position: fixed;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%,-50%);\n  z-index: 1;\n}\n</style>\n"],"sourceRoot":"src"}]}