iff (val) {\n            console.log(val)\n        }\n    },\n\n    watch: {\n        category (value) {\n            this.cat = value\n        },\n\n        quizes (value) {\n            this.QuizArrayLength = value.length\n        }\n    },\n\n}\n</script>"],"sourceRoot":"src/views"}]}