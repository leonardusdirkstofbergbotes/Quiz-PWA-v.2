AAK,EAAE,OAAO,CAAC,KAJD;AAKd,UAAA,QAAQ,EAAE,OAAO,CAAC;AALJ,SAAhB;AAQA,QAAA,QAAQ,CAAC,QAAT,GAAoB,GAApB,CAAwB,+BAAW,QAAQ,CAAC,IAApB,oDAAW,gBAAe,GAA1B,CAAxB,EAAuD,GAAvD,CAA2D,OAA3D,EAT+F,CAS3B;;AAEpE,QAAA,MAAM,CAAC,YAAD,EAAe,OAAf,CAAN,CAX+F,CAWjE;AAC/B,OAZD,EAYG;AAZH,gBAaO,UAAA,KAAK,EAAG;AACb,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAfD,EAD2B,CAgBxB;AACJ;AAlBM,GArBmB;AAyC5B,EAAA,OAAO,EAAE;AACP,IAAA,OADO,mBACE,KADF,EACO;AACZ,aAAO,KAAK,YAAZ;AACD;AAHM;AAzCmB,CAAf,CAAf","sourcesContent":["import Vue from 'vue'\nimport Vuex from 'vuex'\nimport * as firebase from 'firebase'\n\nVue.use(Vuex)\n\nexport default new Vuex.Store({\n  state: {\n    loggedInUser: ''\n  },\n  mutations: {\n    signUserIn (state, payload) {\n      firebase.auth().signInWithEmailAndPassword(payload.email, payload.password).then(response => {\n        // auth the user and get uid \n\n        firebase.database().ref('users/' + response.user?.uid).once('value').then(userInfo => {\n          // get all the info of the user signed in\n\n          console.log('userInfo from database = ' + userInfo)\n        })\n      }) // firebase signin .then ENDS\n      .catch(error => {\n        console.log(error)\n      })// firebase signin .catch ENDS\n    }\n\n  },\n  actions: {\n    createUser ({commit}, payload) {\n      firebase.auth().createUserWithEmailAndPassword(payload.email , payload.password).then(response => {\n        const newUser = {\n          uid: response.user?.uid,\n          name: payload.name,\n          surname: payload.surname,\n          email: payload.email,\n          password: payload.password\n        }\n\n        firebase.database().ref('users/' + response.user?.uid).set(newUser) // store the new user in the database\n      \n        commit('signUserIn', newUser) // if successful then sign user in at the same time\n      }) // firebase auth.then ENDS\n      .catch(error => {\n        console.log(error)\n      }) // firebase auth.catch ENDS\n    }\n  },\n  getters: {\n    getUser (state) {\n      return this.loggedInUser\n    }\n  }\n})\n"],"sourceRoot":""}]}